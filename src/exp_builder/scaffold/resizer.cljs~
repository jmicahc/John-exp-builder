(ns exp-builder.scaffold.resizer)

#_(ui/defcomponent resize-top [state component {:keys [width]}]
  (let [margin-top  -10
        margin-left (/ width 2)
        height      20
        width       (/ width 3)]))

#_(ui/defcomponent resize-bottom [state component {:keys [width height]}])

#_(ui/defcomponent resize-left [state component {:keys [width height]}])

#_(ui/defcomponent resize-right [state component {:keys [width height]}]
  (let []))

#_(ui/defcomponent resizeable [state component {:keys [width height]}])

